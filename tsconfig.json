{
  "include": ["./**/*"],
  "rootDir": "./",
  "baseUrl": "./",
  "paths": {
    "~/*": ["./*"]
  },
  "compilerOptions": {
    "strict": true,
    "esModuleInterop": true,
    "lib": ["dom", "es2015"],
    "jsx": "react-jsx",
    "module": "commonjs",
    "jsxFactoryPath": ["createElement"],
    "jsxModule": "react"
  }
}
